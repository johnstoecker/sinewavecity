    obj_large_1.image_index = 1;
  } else if (current_state == level_menu_states.level_2 ){
    obj_large_2.image_index = 1;
  } else if (current_state == level_menu_states.level_3 ){
    obj_large_3.image_index = 1;
  } else if (current_state == level_menu_states.back ) {
    obj_back.image_index = 1;
  }

  global.state_change_debounce = 5;
}


if gamepad_button_check(0, gp_face1) &amp;&amp; allow_input {
  if (current_state == level_menu_states.level_1) {
    room_goto(room_level1);
  } else if (current_state == level_menu_states.level_2 ){
    room_goto(room_level2);
  } else if (current_state == level_menu_states.level_3 ){
    room_goto(room_level3);
  } else if (current_state == level_menu_states.back ){
    room_goto(room_main_menu);
  }
}

