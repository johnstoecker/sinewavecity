            <string>var left_axis = gamepad_axis_value(0, gp_axislv);

if (left_axis &lt; -0.33 || left_axis &gt; 0.33) &amp;&amp; global.state_change_debounce &lt; 0 &amp;&amp; allow_input {
  if (left_axis &lt; -0.05) {
    current_state += 2;
  }

  if (left_axis &gt; 0.05) {
    current_state += 1;
  } 
     
  obj_new_game.image_index = 0;
  obj_levels.image_index = 0;
  obj_exit.image_index = 0;  

  current_state = current_state % 3;
  if (current_state == states.new_game) {
    obj_new_game.image_index = 1;
  } else if (current_state == states.levels ){
    obj_levels.image_index = 1;
  } else if (current_state == states.game_exit ){
    obj_exit.image_index = 1;
  }

  global.state_change_debounce = 5;
}


if gamepad_button_check(0, gp_face1) &amp;&amp; allow_input {
  if (current_state == states.new_game) {
    room_goto(room_level1);
  } else if (current_state == states.levels ){
    room_goto(room_levels_menu);
  } else if (current_state == states.game_exit ){
    game_end();
  }
}

global.state_change_debounce--;

